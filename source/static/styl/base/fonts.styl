@font-face {
  font-family: 'Circe-Bold';
  src: url('../fonts/CirceBold/Circe-Bold.eot');
  src: url('../fonts/CirceBold/Circe-Bold.woff') format('woff'),
       url('../fonts/CirceBold/Circe-Bold.ttf') format('truetype'),
       url('../fonts/CirceBold/Circe-Bold.eot?#iefix') format('embedded-opentype');
  font-weight: normal;
  font-style: normal;
}

